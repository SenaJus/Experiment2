
#include<iostream>

using namespace std;
int main()
{
	int i, r, c,n;
	
	cout<< " Enter number of column: "; cin>> c;
	cout<< ' ';
	
	cout<< " Enter number of rows: "; cin>> r;
	cout<< ' ';
	for(i = 1 ; i <= c; i++)
	{
		for (n = 1; n <= r; n++)
		{
		
		cout<< " * ";
	}
	cout<< endl;
}
	return 0;
		
	}
